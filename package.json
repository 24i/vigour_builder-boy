{
  "name": "builder-boy",
  "description": "Build and watch es modules FAST",
  "version": "0.0.27",
  "main": "lib/index.js",
  "scripts": {
    "test": "node test/index.js",
    "dev": "nodemon --ignore dist/* --harmony-async-await test/index.js",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "builder-boy": "node ./bin/cmd.js",
    "precommit": "npm run test && standard --fix"
  },
  "precommit": "precommit",
  "bin": {
    "builder-boy": "bin/cmd.js"
  },
  "standard": {
    "ignore": [
      "test/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/builder-boy.git"
  },
  "keywords": [
    "fast",
    "js",
    "builder"
  ],
  "nyc": {
    "instrument": true
  },
  "dependencies": {
    "acorn": "^4.0.4",
    "acorn-es7-plugin": "^1.1.5",
    "async-to-gen": "^1.3.2",
    "brisky-stamp": "^4.0.0",
    "brisky-struct": "unstable",
    "buble": "^0.15.2",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "is-builtin-module": "^1.0.0",
    "node-fetch": "^1.6.3",
    "promise-polyfill": "^6.0.2",
    "regenerator": "^0.9.5",
    "regenerator-runtime": "^0.10.1",
    "resolve": "^1.2.0",
    "string-hash": "^1.1.1",
    "vigour-ua": "^3.1.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "strip-formatting ": "^1.0.2",
    "coveralls": "^2.11.9",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.1.0",
    "tape": "4.6.0",
    "brisky-render": "2.2.8"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/builder-boy/issues"
  },
  "homepage": "https://github.com/vigour-io/builder-boy#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io"
  ]
}
