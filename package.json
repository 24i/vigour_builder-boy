{
  "name": "builder-boy",
  "description": "Build and watch es modules FAST",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "pretest": "standard",
    "test": "node test/index.js",
    "dev": "nodemon --harmony-async-await test/index.js",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/builder-boy.git"
  },
  "keywords": [
    "fast",
    "js",
    "builder"
  ],
  "nyc": {
    "include": [
      "dist/index.js"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "brisky-struct": "git://github.com/vigour-io/brisky-struct.git#feature/enhancements",
    "chokidar": "^1.6.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "pre-commit": "^1.1.3",
    "source-map-support": "^0.4.7",
    "standard": "^8.1.0",
    "tape": "4.6.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/builder-boy/issues"
  },
  "homepage": "https://github.com/vigour-io/builder-boy#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io"
  ]
}
